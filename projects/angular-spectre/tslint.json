{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "ngs",
      "camelCase"
    ],
    "component-selector": [
      true,
      [
        "element",
        "attribute"
      ],
      "ngs",
      "kebab-case"
    ],
    "no-input-rename": [
      false
    ],
    "no-output-on-prefix": [
      false
    ]
  }
}
